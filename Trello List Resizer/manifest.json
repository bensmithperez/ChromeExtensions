{
    "manifest_version": 2,
    "name": "Trello List Resizer",
    "content_scripts": [
        {
         "matches": [
            "*://*.trello.com/*"
         ],
         "css": ["estiloNuevo.css"],
         "run_at" : "document_start"
        }
    ],
    "browser_action": {
    "default_popup": "popup.html"
  },
    "version":"0.1"
}