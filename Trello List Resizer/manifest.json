{
    "manifest_version": 2,
    
    "name": "Trello List Resizer",
    "description": "Simple extension for wider Trello lists.",
    "version":"0.1",
    "content_scripts": [
        {
         "matches": [
            "*://*.trello.com/*"
         ],
         "css": ["estiloNuevo.css"],
         "run_at" : "document_start"
        }
    ]
}